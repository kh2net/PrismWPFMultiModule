<MenuItem
    x:Class="Module1.TabHeaderModule1UserControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Module1"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:mvvm="http://prismlibrary.com/"
    xmlns:viewmodels="clr-namespace:Module1.ViewModels"
    d:DataContext="{d:DesignInstance Type=viewmodels:TabHeaderModule1ViewModel}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mvvm:ViewModelLocator.AutoWireViewModel="True"
    Header="View1-Menu"
    mc:Ignorable="d">
    <MenuItem Command="{Binding OpenView1Command}" Header="view1" />
</MenuItem>
